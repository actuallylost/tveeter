#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lost/Projects/tveeter-api/node_modules/.pnpm/prettier@2.8.7/node_modules/prettier/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/prettier@2.8.7/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/node_modules:/Users/lost/Projects/tveeter-api/node_modules:/Users/lost/Projects/node_modules:/Users/lost/node_modules:/Users/node_modules:/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/eslint-plugin-prettier@4.2.1_c5975de6a481b4ab961ce2cab41cfd63/node_modules/eslint-plugin-prettier/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/eslint-plugin-prettier@4.2.1_c5975de6a481b4ab961ce2cab41cfd63/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/prettier@2.8.7/node_modules/prettier/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/prettier@2.8.7/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/node_modules:/Users/lost/Projects/tveeter-api/node_modules:/Users/lost/Projects/node_modules:/Users/lost/node_modules:/Users/node_modules:/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/eslint-plugin-prettier@4.2.1_c5975de6a481b4ab961ce2cab41cfd63/node_modules/eslint-plugin-prettier/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/eslint-plugin-prettier@4.2.1_c5975de6a481b4ab961ce2cab41cfd63/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../prettier@2.8.7/node_modules/prettier/bin-prettier.js" "$@"
else
  exec node  "$basedir/../../../../../prettier@2.8.7/node_modules/prettier/bin-prettier.js" "$@"
fi
