#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lost/Projects/tveeter-api/node_modules/.pnpm/typescript@4.9.5/node_modules/typescript/bin/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/typescript@4.9.5/node_modules/typescript/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/typescript@4.9.5/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/node_modules:/Users/lost/Projects/tveeter-api/node_modules:/Users/lost/Projects/node_modules:/Users/lost/node_modules:/Users/node_modules:/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/fork-ts-checker-webpack-plugin@8.0.0_typescript@4.9.5+webpack@5.76.2/node_modules/fork-ts-checker-webpack-plugin/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/fork-ts-checker-webpack-plugin@8.0.0_typescript@4.9.5+webpack@5.76.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/typescript@4.9.5/node_modules/typescript/bin/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/typescript@4.9.5/node_modules/typescript/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/typescript@4.9.5/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/node_modules:/Users/lost/Projects/tveeter-api/node_modules:/Users/lost/Projects/node_modules:/Users/lost/node_modules:/Users/node_modules:/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/fork-ts-checker-webpack-plugin@8.0.0_typescript@4.9.5+webpack@5.76.2/node_modules/fork-ts-checker-webpack-plugin/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/fork-ts-checker-webpack-plugin@8.0.0_typescript@4.9.5+webpack@5.76.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@4.9.5/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../../typescript@4.9.5/node_modules/typescript/bin/tsserver" "$@"
fi
