#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lost/Projects/tveeter-api/node_modules/.pnpm/@nestjs+cli@9.3.0/node_modules/@nestjs/cli/bin/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/@nestjs+cli@9.3.0/node_modules/@nestjs/cli/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/@nestjs+cli@9.3.0/node_modules/@nestjs/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/@nestjs+cli@9.3.0/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/node_modules:/Users/lost/Projects/tveeter-api/node_modules:/Users/lost/Projects/node_modules:/Users/lost/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/@nestjs+cli@9.3.0/node_modules/@nestjs/cli/bin/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/@nestjs+cli@9.3.0/node_modules/@nestjs/cli/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/@nestjs+cli@9.3.0/node_modules/@nestjs/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/@nestjs+cli@9.3.0/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/node_modules:/Users/lost/Projects/tveeter-api/node_modules:/Users/lost/Projects/node_modules:/Users/lost/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nest.js" "$@"
else
  exec node  "$basedir/../../bin/nest.js" "$@"
fi
