#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lost/Projects/tveeter-api/node_modules/.pnpm/ts-node@10.9.1_08ccfdaf59301cf71800ade926127531/node_modules/ts-node/dist/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/ts-node@10.9.1_08ccfdaf59301cf71800ade926127531/node_modules/ts-node/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/ts-node@10.9.1_08ccfdaf59301cf71800ade926127531/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/node_modules:/Users/lost/Projects/tveeter-api/node_modules:/Users/lost/Projects/node_modules:/Users/lost/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/ts-node@10.9.1_08ccfdaf59301cf71800ade926127531/node_modules/ts-node/dist/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/ts-node@10.9.1_08ccfdaf59301cf71800ade926127531/node_modules/ts-node/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/ts-node@10.9.1_08ccfdaf59301cf71800ade926127531/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/node_modules:/Users/lost/Projects/tveeter-api/node_modules:/Users/lost/Projects/node_modules:/Users/lost/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin.js" "$@"
else
  exec node  "$basedir/../../dist/bin.js" "$@"
fi
