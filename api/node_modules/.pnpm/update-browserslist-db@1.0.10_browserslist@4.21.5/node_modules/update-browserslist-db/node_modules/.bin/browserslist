#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lost/Projects/tveeter-api/node_modules/.pnpm/browserslist@4.21.5/node_modules/browserslist/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/browserslist@4.21.5/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/node_modules:/Users/lost/Projects/tveeter-api/node_modules:/Users/lost/Projects/node_modules:/Users/lost/node_modules:/Users/node_modules:/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/update-browserslist-db@1.0.10_browserslist@4.21.5/node_modules/update-browserslist-db/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/update-browserslist-db@1.0.10_browserslist@4.21.5/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/browserslist@4.21.5/node_modules/browserslist/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/browserslist@4.21.5/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/node_modules:/Users/lost/Projects/tveeter-api/node_modules:/Users/lost/Projects/node_modules:/Users/lost/node_modules:/Users/node_modules:/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/update-browserslist-db@1.0.10_browserslist@4.21.5/node_modules/update-browserslist-db/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/update-browserslist-db@1.0.10_browserslist@4.21.5/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../browserslist@4.21.5/node_modules/browserslist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../browserslist@4.21.5/node_modules/browserslist/cli.js" "$@"
fi
