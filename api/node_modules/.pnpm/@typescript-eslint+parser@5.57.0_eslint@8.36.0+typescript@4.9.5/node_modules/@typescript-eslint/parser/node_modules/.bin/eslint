#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lost/Projects/tveeter-api/node_modules/.pnpm/eslint@8.36.0/node_modules/eslint/bin/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/eslint@8.36.0/node_modules/eslint/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/eslint@8.36.0/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/node_modules:/Users/lost/Projects/tveeter-api/node_modules:/Users/lost/Projects/node_modules:/Users/lost/node_modules:/Users/node_modules:/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/@typescript-eslint+parser@5.57.0_eslint@8.36.0+typescript@4.9.5/node_modules/@typescript-eslint/parser/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/@typescript-eslint+parser@5.57.0_eslint@8.36.0+typescript@4.9.5/node_modules/@typescript-eslint/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/@typescript-eslint+parser@5.57.0_eslint@8.36.0+typescript@4.9.5/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/eslint@8.36.0/node_modules/eslint/bin/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/eslint@8.36.0/node_modules/eslint/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/eslint@8.36.0/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/node_modules:/Users/lost/Projects/tveeter-api/node_modules:/Users/lost/Projects/node_modules:/Users/lost/node_modules:/Users/node_modules:/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/@typescript-eslint+parser@5.57.0_eslint@8.36.0+typescript@4.9.5/node_modules/@typescript-eslint/parser/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/@typescript-eslint+parser@5.57.0_eslint@8.36.0+typescript@4.9.5/node_modules/@typescript-eslint/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/@typescript-eslint+parser@5.57.0_eslint@8.36.0+typescript@4.9.5/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../eslint@8.36.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../../../eslint@8.36.0/node_modules/eslint/bin/eslint.js" "$@"
fi
