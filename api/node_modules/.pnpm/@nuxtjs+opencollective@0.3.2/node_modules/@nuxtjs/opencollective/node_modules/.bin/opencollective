#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lost/Projects/tveeter-api/node_modules/.pnpm/@nuxtjs+opencollective@0.3.2/node_modules/@nuxtjs/opencollective/bin/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/@nuxtjs+opencollective@0.3.2/node_modules/@nuxtjs/opencollective/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/@nuxtjs+opencollective@0.3.2/node_modules/@nuxtjs/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/@nuxtjs+opencollective@0.3.2/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/node_modules:/Users/lost/Projects/tveeter-api/node_modules:/Users/lost/Projects/node_modules:/Users/lost/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/@nuxtjs+opencollective@0.3.2/node_modules/@nuxtjs/opencollective/bin/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/@nuxtjs+opencollective@0.3.2/node_modules/@nuxtjs/opencollective/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/@nuxtjs+opencollective@0.3.2/node_modules/@nuxtjs/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/@nuxtjs+opencollective@0.3.2/node_modules:/Users/lost/Projects/tveeter-api/node_modules/.pnpm/node_modules:/Users/lost/Projects/tveeter-api/node_modules:/Users/lost/Projects/node_modules:/Users/lost/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/opencollective.js" "$@"
else
  exec node  "$basedir/../../bin/opencollective.js" "$@"
fi
